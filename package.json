{
    "name": "skv-reporter",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "test": "mocha -r ts-node/register src/**/*.test.ts",
        "coverage": "nyc --check-coverage --lines=70 mocha -r ts-node/register src/**/*.test.ts",
        "lint": "eslint src/**/*.ts",
        "format": "eslint src/**/*.ts --fix",
        "nuke": "rm -rfv dist coverage node_modules package-lock.json yarn.lock .nyc_output"
    },
    "dependencies": {
        "@mui/material": "^5.4.0",
        "@mui/styled-engine-sc": "^5.3.0",
        "@mui/x-data-grid": "^5.5.0",
        "ajv": "^8.10.0",
        "date-fns": "^2.20.2",
        "dotenv": "^8.2.0",
        "fast-xml-parser": "^3.19.0",
        "multer": "^1.4.2",
        "next": "12.0.10",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.12",
        "@types/chai-as-promised": "^7.1.3",
        "@types/lodash": "^4.14.168",
        "@types/mocha": "^8.0.1",
        "@types/node": "17.0.15",
        "@types/react": "17.0.39",
        "@types/sinon": "^9.0.4",
        "@typescript-eslint/eslint-plugin": "^5.8.0",
        "@typescript-eslint/parser": "^5.8.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "lodash": "^4.17.21",
        "mocha": "^8.1.0",
        "sinon": "^9.0.3",
        "ts-node": "^8.10.2",
        "ts-sinon": "^1.2.0",
        "typescript": "^4.5.5",
        "eslint": "^8.8.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-config-next": "12.0.10",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "nyc": "^15.1.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged lint-staged",
            "pre-push": "yarn test"
        }
    },
    "lint-staged": {
        "*.{js,ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    },
    "nyc": {
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "**/*.test.ts"
        ],
        "extension": [
            ".ts"
        ],
        "reporter": [
            "json-summary",
            "text-summary",
            "html"
        ],
        "sourceMap": true,
        "instrument": true
    }
}
